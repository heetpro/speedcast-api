{
  "name": "speedcast-api",
  "version": "1.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:types": "tsc --project tsconfig.types.json",
    "clean": "rimraf dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "publish-package": "npm run build && npm publish"
  },
  "keywords": [
    "speedcast",
    "http",
    "client",
    "api",
    "fetch",
    "typescript",
    "cache",
    "rate-limit",
    "retry",
    "promise",
    "async",
    "axios",
    "request",
    "rest",
    "ajax"
  ],
  "author": {
    "name": "Heet Vavadiya",
    "email": "heetvavadiya456@gmail.com",
    "url": "https://github.com/heetpro"
  },
  "license": "ISC",
  "description": "Speedcast API is a lightweight and fast API client for Next.js and Node.js. It provides a simple and intuitive interface for making HTTP requests and handling responses.",
  "repository": {
    "type": "git",
    "url": "https://github.com/heetpro/speedcast-api"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "jest-extended": "^6.0.0",
    "jest-fixed-jsdom": "^0.0.9",
    "msw": "^2.10.2",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "events": "^3.3.0",
    "next": "^15.3.3",
    "node-fetch": "^2.7.0"
  }
}
